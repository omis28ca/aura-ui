{
  "name": "aura-hygienics-frameworkless",
  "version": "1.0.0",
  "description": "Aura Hygienics",
  "author": "Aura Hygienics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/electron/electron-quick-start"
  },
  "main": "main.js",
  "scripts": {
    "app": "electron . --no-sandbox",
    "app-build": "electron-builder --dir",
    "app:dist": "electron-builder",
	"dev": "vite --force --host",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "preview": "vite preview"
  },
  "keywords": [ "Electron"  ],
  "build": {
    "productName": "electron-quick-start",
    "appId": "electron-quick-start",
    "mac": {
      "target": [
        {
          "target": "default",
          "arch": "universal"
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^25.1.1",
    "electron-builder": "^24.4.0",
	"@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "postcss": "^8.4.38",
    "sass": "^1.72.0",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "jsrender": "^1.0.13",
    "mqtt": "^4.3.7",
    "pouchdb": "^8.0.1",
    "serialport": "^11.0.0",
	"@coreui/chartjs": "^4.0.0",
    "@coreui/coreui": "^5.0.0",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-vue": "2.0.0",
    "@coreui/utils": "^2.0.2",
    "@coreui/vue": "^5.0.0",
    "@coreui/vue-chartjs": "^3.0.0",
    "@popperjs/core": "^2.11.8",
    "chart.js": "^4.4.2",
    "multi-range-slider-vue": "^1.1.4",
    "pinia": "^2.1.7",
    "simplebar-vue": "^2.3.3",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  }
}
